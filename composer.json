{
    "name": "nikkanetiya/laravel-color-palette",
    "description": "Laravel Wrapper for `ksubileau/color-thief-php`. Grabs the dominant color or a representative color palette from an image. Uses PHP and GD or Imagick.",
    "homepage": "https://github.com/nikkanetiya/laravel-color-palette",
    "keywords": ["laravel","color-palette","image-colors","color-thief","php", "gd"],
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Nikunj Kanetiya",
            "email": "kanetiya@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "ksubileau/color-thief-php": "^2.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "NikKanetiya\\LaravelColorPalette\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NikKanetiya\\LaravelColorPalette\\ColorPaletteServiceProvider"
            ],
            "aliases": {
                "ColorPalette": "NikKanetiya\\LaravelColorPalette\\ColorPaletteFacade"
            }
        }
    }
}
